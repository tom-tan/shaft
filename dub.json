{
	"authors": [
		"Tomoya Tanjo"
	],
	"buildTypes": {
		"release-static": {
			"buildOptions": [
				"releaseMode",
				"inline",
				"optimize"
			],
			"dflags-posix-ldc": [
				"-static"
			]
		}
	},
	"preBuildCommands": [
		"mkdir -p resources",
		"test -d .git && git describe --always --dirty > resources/version",
		"test -d .git || cp VERSION resources/version"
	],
	"stringImportPaths": [
		"resources"
	],
	"copyright": "Copyright Â© 2022, Tomoya Tanjo",
	"dependencies": {
		"schema-salad-d": "~>0.6.1"
	},
	"description": "A workflow engine for CommandLineTool in local machine",
	"homepage": "https://github.com/tom-tan/shaft",
	"license": "Apache-2.0",
	"name": "shaft",
	"targetPath": "bin",
	"targetType": "executable"
}
